<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.elapid.spring01.dao.LogDao"> <!-- 자바 파일이름 써야됨  -->
	<insert id="visitorCount">
		INSERT INTO VISITOR(IP,FUNNEL,DATE) VALUES(#{param1},#{param2},NOW())
	</insert>
	<select id="returnLatestDateTime" resultType="com.elapid.spring01.dto.DatesDto" >
		SELECT DATE_FORMAT(DATE,'%y%m%d%k%i%s') AS DATE,DATE_FORMAT(NOW(),'%y%m%d%k%i') AS NOWDATE FROM VISITOR WHERE IP = #{param1} ORDER BY DATE DESC LIMIT 1
	</select>
	
</mapper>